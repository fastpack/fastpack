{
  "name": "fastpack",
  "version": "0.5.1",
  "description": "Pack JS code into a single bundle fast & easy.",
  "main": "index.js",
  "esy": {
    "build": [
      "jbuilder build"
    ],
    "install": [
      "esy-installer Fastpack.install"
    ],
    "buildsInSource": "_build"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastpack/fastpack.git"
  },
  "author": [
    "Oleksiy Golovko <alexei.golovko@gmail.com>",
    "Andrey Popp <8mayday@gmail.com> (http://andreypopp.com/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastpack/fastpack/issues"
  },
  "homepage": "https://github.com/fastpack/fastpack#readme",
  "dependencies": {
    "@esy-ocaml/esy-installer": "*",
    "@opam/cmdliner": "1.0.2",
    "@opam/containers": "1.3",
    "@opam/fileutils": "0.5.3",
    "@opam/jbuilder": "*",
    "@opam/lwt": "3.3.0",
    "@opam/merlin": "*",
    "@opam/sequence": "*",
    "@opam/ocp-indent": "*",
    "@opam/sedlex": "^1.99.4",
    "@opam/utop": "^2.0.2",
    "@opam/wtf8": "^1.0.1",
    "@opam/yojson": "1.4.0",
    "@opam/logs": "0.6.2",
    "@opam/fmt": "0.8.5",
    "@opam/re": "1.7.1",
    "@opam/ppx_expect": "*",
    "@opam/ppx_let": "*",
    "@opam/ppx_deriving": "^4.2.1",
    "ocaml": "~4.6.1"
  }
}
